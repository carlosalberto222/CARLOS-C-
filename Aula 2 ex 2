#include <stdio.h>

int main() {
    // [[][][][]]
    int idade = 20;           
    float altura = 1.86;            
    char genero = 'M';        
    char nome[] = "Felipe";       

    // Exibindo os valores das vari√°veis individualmente
    printf("%d\n", idade);
    printf("%.2f\n", altura); 
    printf("%c\n", genero); 
    printf("%s\n", nome); 

    printf("Idade: %d\n", idade);
    printf("Altura: %.2f\n", altura); 
    printf("Genero: %c\n", genero); 
    printf("Nome: %s\n", nome); 
    
    return 0;
}
